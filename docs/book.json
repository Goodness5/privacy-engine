{
  "title": "Privacy Engine Documentation",
  "description": "Multi-protocol cryptographic library for secure message encryption and key management",
  "author": "Privacy Engine Team",
  "language": "en",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "theme-default",
    "highlight",
    "search",
    "github",
    "copy-code-button",
    "expandable-chapters",
    "back-to-top-button",
    "favicon",
    "toc",
    "anchorjs"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true,
      "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
      }
    },
    "github": {
      "url": "https://github.com/your-repo/privacy-engine"
    },
    "favicon": {
      "shortcut": "assets/favicon.ico",
      "bookmark": "assets/favicon.ico",
      "appleTouch": "assets/apple-touch-icon.png",
      "appleTouchMore": {
        "120x120": "assets/apple-touch-icon-120x120.png",
        "180x180": "assets/apple-touch-icon-180x180.png"
      }
    },
    "toc": {
      "addClass": true,
      "className": "toc"
    },
    "anchorjs": {
      "selector": "h1, h2, h3, h4, h5, h6",
      "placement": "left",
      "visible": "hover",
      "icon": "ยง",
      "class": "custom-anchor"
    }
  },
  "variables": {
    "themeColor": "#667eea",
    "themeColorSecondary": "#764ba2",
    "themeColorAccent": "#f5576c"
  }
}
